{
  "env": {
    "es2021": true
  },
  "extends": [
    "plugin:react-hooks/recommended",
    "react-app",
    "react-app/jest",
    "prettier"
  ],
  "rules": {
    "no-duplicate-imports": "error",
    "camelcase": "error",
    "eqeqeq": ["error", "smart"],
    "yoda": ["error", "never", { "onlyEquality": true }]
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
